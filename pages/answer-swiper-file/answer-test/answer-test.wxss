/* 新增内容 */
.popup-box{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999999;
  background: #fff;

  padding: 0 38rpx;
  box-sizing: border-box;
  display: none;
}
.block{
  display: block !important;
}
.pop-con{
  width: 100%;
  height: 92vh;
  overflow: auto;

}
.pop-btn-box{
  width: 100%;
  height: 7vh;

  display: flex;
  align-items: center;
  justify-content: center;
}
.pop-btn{
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  color: #fff;
  height: 80%;
  width: 50%;
  border-radius:30px;
  background:linear-gradient(90deg,rgba(56,164,239,1) 0%,rgba(0,102,173,1) 100%);
}
.pop-con-title{
  font-size: 40rpx;
  margin-top: 14rpx;
  margin-bottom: 38rpx;
  width: 100%;
  text-align: center;
}
.pop-c-sub{
  display: flex;
  flex-wrap: wrap;
}

.pop-c-sub-i{
  margin-bottom:30rpx ;
  width: calc((100% - 224rpx)/6);
  border-radius: 50%;
  border:1px solid rgba(153,153,153,1);
  font-size: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}

.pop-c-sub-i::before{content: "";padding-top: 100%;}

.pop-c-sub-i:not(:nth-child(6n)){
  margin-right:44rpx ;

}
