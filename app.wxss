/**app.wxss**/
page{
  background: #FFF;
  color: #333333;

}
.container {
  height: 100%;
  width: 100%;
  box-sizing: border-box;

} 
image{
  display: block;
  }
  .user-img,.index-swiper-img,.sele-img,.swiper-item-img,.swiper-img,.img-phtot,.img-photo {
    background-color: rgb(223, 239, 253) !important;
  }
.replace{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  color: #999999;
  text-align: center;
  font-size: 14px;
}

.hidden1 {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.hidden2 {
  	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
}
.clear::after,
.clear::before {
  content: "";
  width: 0;
  height: 0;
  display: block;
  clear: both;
}
.rel{
  position: relative;
}
.login{
  position: absolute;
  bottom: 0;
  top: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 536rpx;
  height: 100rpx;
 background:linear-gradient(90deg,rgba(56,164,239,1) 0%,rgba(0,102,173,1) 100%);
  border-radius: 50rpx;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
  font-size: 32rpx;
}
.nav{
  justify-content: center!important;
}
.home-img {
  height: 458rpx;
  width: 100%;
}
